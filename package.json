{
  "name": "cdflow2-action",
  "scripts": {
    "build": "ncc build src/index.ts -o dist"
  },
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/tool-cache": "^1.7.1",
    "@actions/exec": "^1.1.0",
    "@actions/io": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.26",
    "@vercel/ncc": "^0.33.3",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "ts-config": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "roots": [
      "src"
    ]
  }
}
